#include <stdio.h>
int main() {
	int a, b, c, x;
	printf("Start Now\n");
	x = 1;
	while (x < 1000) {
		a = x / 100;
		b = x / 10 - x / 100 * 10;
		c = x % 10;
		if (x % 3 == 0)
			printf("%d   no\n", x);
		else if (a == 3 || b == 3 || c == 3)
			printf("%d   no\n", x);
		else printf("%d   yes\n",x);
		x = x + 1;
	}
	return 0;
}
